{
    "name": "maxime-rainville/silverstripe-password-manager-helper",
    "description": "Tweak Siverstripe CMS to work better with password managers",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4",
        "silverstripe/admin": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MaximeRainville\\SilverstripePasswordManagerHelper\\": "src/",
            "MaximeRainville\\SilverstripePasswordManagerHelper\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
